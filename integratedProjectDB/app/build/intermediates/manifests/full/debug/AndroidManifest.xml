<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alamkanak.weekview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.cse110.apk404.myCalendar.myCalendarApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cse110.apk404.myCalendar.MainActivity"
            android:configChanges="orientation|screenSize"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cse110.apk404.myCalendar.DetailActivity"
            android:label="@string/app_detail_activity"
            android:parentActivityName="com.cse110.apk404.myCalendar.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.cse110.apk404.myCalendar.AddEventActivity"
            android:label="@string/app_add_event_activity"
            android:parentActivityName="com.cse110.apk404.myCalendar.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.cse110.apk404.myCalendar.LoginActivity"
            android:parentActivityName="com.cse110.apk404.myCalendar.MainActivity"
            android:screenOrientation="portrait" >
        </activity>

        <!-- <activity -->
        <!-- android:name=".sampleReference.BasicActivity" -->
        <!-- android:label="@string/title_activity_basic"> -->
        <!-- </activity> -->
        <!-- <activity -->
        <!-- android:name=".sampleReference.AsynchronousActivity" -->
        <!-- android:label="@string/title_activity_asynchronous"> -->
        <!-- </activity> -->
    </application>

</manifest>